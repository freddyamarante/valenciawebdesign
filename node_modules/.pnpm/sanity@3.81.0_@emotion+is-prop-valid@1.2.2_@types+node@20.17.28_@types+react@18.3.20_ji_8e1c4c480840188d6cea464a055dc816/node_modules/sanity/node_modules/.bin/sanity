    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules/sanity/bin/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules/sanity/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules/sanity/bin/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules/sanity/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/sanity@3.81.0_@emotion+is-prop-valid@1.2.2_@types+node@20.17.28_@types+react@18.3.20_ji_8e1c4c480840188d6cea464a055dc816/node_modules:/Users/freddyamarante/Documents/valenciawebdesign/node_modules/.pnpm/node_modules:$NODE_PATH"
